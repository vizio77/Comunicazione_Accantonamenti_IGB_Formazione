                <ObjectStatus title="Desc. Titolo" text="{modelPosFin>/detailAnagrafica/DESC_TITOLO}" class="textBold"/>
                <ObjectStatus title="Desc. Categoria" text="{modelPosFin>/detailAnagrafica/DESC_CATEGORIA}" class="textBold"/>
                <ObjectStatus title="Desc. Ce2" text="{modelPosFin>/detailAnagrafica/DESC_CE2}" class="textBold"/>
				<ObjectStatus title="Desc. Ce3" text="{modelPosFin>/detailAnagrafica/DESC_CE3}" class="textBold"/>
