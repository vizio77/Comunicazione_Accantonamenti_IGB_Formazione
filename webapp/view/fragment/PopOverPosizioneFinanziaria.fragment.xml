<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Popover title="{modelPosFin>/posFin}" class="sapUiContentPadding" placement="Bottom" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
		<content>
			<l:VerticalLayout class="sapUiSmallMargin">
                <ObjectStatus title="Amministrazione" text="{modelPosFin>/detailAnagrafica/AMMINISTAZIONE}" class="textBold"/>
                <ObjectStatus title="Capitolo" text="{modelPosFin>/detailAnagrafica/CAPITOLO}" class="textBold"/>
                <ObjectStatus title="PG" text="{modelPosFin>/detailAnagrafica/pg}" class="textBold"/>
                <ObjectStatus title="Missione" text="{modelPosFin>/detailAnagrafica/MISSIONE}" class="textBold"/>
                <ObjectStatus title="Programma" text="{modelPosFin>/detailAnagrafica/PROGRAMMA}" class="textBold"/>
                <ObjectStatus title="Azione" text="{modelPosFin>/detailAnagrafica/AZIONE}" class="textBold"/>
                <ObjectStatus title="Titolo" text="{modelPosFin>/detailAnagrafica/TITOLO}" class="textBold"/>
                <ObjectStatus title="Categoria" text="{modelPosFin>/detailAnagrafica/CATEGORIA}" class="textBold"/>
                <ObjectStatus title="C.E.2" text="{modelPosFin>/detailAnagrafica/CE2}" class="textBold"/>
				<ObjectStatus title="C.E.3" text="{modelPosFin>/detailAnagrafica/CE3}" class="textBold"/>
			</l:VerticalLayout>
			<l:VerticalLayout class="sapUiSmallMargin">
                <ObjectStatus title="Desc. Amm" text="{modelPosFin>/detailAnagrafica/DESC_AMMINISTAZIONE}" class="textBold"/>
