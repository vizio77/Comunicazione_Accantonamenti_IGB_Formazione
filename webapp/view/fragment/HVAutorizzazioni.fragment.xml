<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
>
<TableSelectDialog title="Cerca Autorizzazione" search="" confirm="handleConfirmAuth" cancel=".onClose"
		items="{modelPosFin>/elencoAuth/}">
		<columns>
			<Column width="14rem">
				<header>
					<Text text="Descrizione compatta"/>
				</header>
			</Column>
			<Column width="8rem">
				<header>
					<Text text="Attivazione"/>
				</header>
			</Column>
			<Column width="8rem">
				<header>
					<Text text="Scadenza"/>
				</header>
			</Column>
			<Column width="8rem">
				<header>
					<Text text="Classificazione"/>
				</header>
			</Column>
			<Column width="8rem">
				<header>
					<Text text="Natura"/>
				</header>
			</Column>
			<Column width="8rem">
				<header>
					<Text text="Disponibilità 2023"/>
				</header>
			</Column>
			<Column width="8rem">
				<header>
					<Text text="Disponibilità 2024"/>
				</header>
			</Column>
			<Column width="8rem">
				<header>
					<Text text="Disponibilità 2025"/>
				</header>
			</Column>
			<Column width="8rem">
				<header>
					<Text text=""/>
				</header>
			</Column>
		</columns>
		<ColumnListItem>
			<cells>
				<Text text="{modelPosFin>DescrizioneCompatta}"/>
					<Text text="{modelPosFin>DescNickname}"/>
					<Text text="{modelPosFin>Attivazione}"/>
					<Text text="{modelPosFin>Scadenza}"/>
					<Text text="{modelPosFin>DescClassificazione}"/>
					<Text text="{modelPosFin>Natura}"/>
					<Text text="{modelPosFin>ImpoDispCurrent}"/>
					<Text text="{modelPosFin>ImpoDispPlusOne}"/>
					<Text text="{modelPosFin>ImpoDispPlusTwo}"/>
					<Text text="{modelPosFin>Tipologia}"/>
					<Text text="{modelPosFin>IndicatMonit}"/>
			</cells>
		</ColumnListItem>
	</TableSelectDialog>
</c:FragmentDefinition>