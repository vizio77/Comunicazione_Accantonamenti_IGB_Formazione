<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<TableSelectDialog title="Cerca" search="handleSearchFinan" confirm="onConfirmTablePosFin" cancel="handleCloseFinan"
		items="{modelPosFin>/elencoPosFin/}">
		<columns>
			<Column width="14rem">
				<header>
					<Text text="{i18n>labelPosFin}"/>
				</header>
			</Column>
			<Column width="8rem">
				<header>
					<Text text="{i18n>Amministrazione}"/>
				</header>
			</Column>
			<Column width="12rem">
				<header>
					<Text text="{i18n>descAmm}"/>
				</header>
			</Column>
			<Column width="5rem">
				<header>
					<Text text="{i18n>mission}"/>
				</header>
			</Column>
			<Column width="12rem">
				<header>
					<Text text="{i18n>descrMissione}"/>
				</header>
			</Column>
			<Column width="7rem">
				<header>
					<Text text="{i18n>programma}"/>
				</header>
			</Column>
			<Column width="12rem">
				<header>
					<Text text="{i18n>descrProgramma}"/>
				</header>
			</Column>
			<Column width="5rem">
				<header>
					<Text text="{i18n>azione}"/>
				</header>
			</Column>
			<Column width="12rem">
				<header>
					<Text text="{i18n>descrAzione}"/>
				</header>
			</Column>
			<Column width="5rem">
				<header>
					<Text text="{i18n>Capitolo}"/>
				</header>
			</Column>
			<Column width="12rem">
				<header>
					<Text text="{i18n>descrCapitolo}"/>
				</header>
			</Column>
			<Column width="5rem">
				<header>
					<Text text="{i18n>Pg}"/>
				</header>
			</Column>
			<Column width="12rem">
				<header>
					<Text text="{i18n>descrPg}"/>
				</header>
			</Column>
			<Column width="5rem">
				<header>
					<Text text="{i18n>categoria}"/>
				</header>
			</Column>
			<Column width="12rem">
				<header>
					<Text text="{i18n>descrCategoria}"/>
				</header>
			</Column>
			<Column width="5rem">
				<header>
					<Text text="{i18n>ce}"/>
				</header>
			</Column>
			<Column width="12rem">
				<header>
					<Text text="{i18n>descrCe}"/>
				</header>
			</Column>
			<Column width="5rem">
				<header>
					<Text text="{i18n>ce3}"/>
				</header>
			</Column>
			<Column width="12rem">
				<header>
					<Text text="{i18n>descrCe3}"/>
				</header>
			</Column>
		</columns>
		<ColumnListItem>
			<cells>
				<Text text="{modelPosFin>Fipex}"/>
				<Text text="{modelPosFin>Prctr}"/>
				<Text text="{modelPosFin>DESC_AMMINISTAZIONE}"/>
				<Text text="{modelPosFin>Cdr}"/>
				<Text text="{modelPosFin>Ragioneria}"/>
				<Text text="{modelPosFin>DescRagioneria}"/>
				<Text text="{modelPosFin>Missione}"/>
				<Text text="{modelPosFin>DESC_MISSIONE}"/>
				<Text text="{modelPosFin>Programma}"/>
				<Text text="{modelPosFin>DESC_PROGRAMMA}"/>
				<Text text="{modelPosFin>Azione}"/>
				<Text text="{modelPosFin>DESC_AZIONE}"/>
				<Text text="{modelPosFin>Capitolo}"/>
				<Text text="{modelPosFin>DESC_CAPITOLO}"/>
				<Text text="{modelPosFin>Pg}"/>
				<Text text="{modelPosFin>DESC_PG_ESTESA}"/>
				<Text text="{modelPosFin>Titolo}"/>
				<Text text="{modelPosFin>DescTitolo}"/>
				<Text text="{modelPosFin>Categoria}"/>
				<Text text="{modelPosFin>DESC_CATEGORIA}"/>
				<Text text="{modelPosFin>Ce2}"/>
				<Text text="{modelPosFin>DESC_CE2}"/>
				<Text text="{modelPosFin>Ce3}"/>
				<Text text="{modelPosFin>DESC_CE3}"/>
			</cells>
		</ColumnListItem>
