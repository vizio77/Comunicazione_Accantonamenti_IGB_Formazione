					<fb:FilterBar  id="filterbar" reset="onReset" search="onPressAvvio" useToolbar="false"
						 showFilterConfiguration="true"  showRestoreButton="true" showClearButton="true" showClearOnFB="true" clear="onReset" class="stileEsercizio">
						<fb:filterGroupItems >
						<!-- lt obbligatori -->
							<fb:FilterGroupItem groupName="Essential" name="A" groupTitle="{i18n>Essential}" partOfCurrentVariant="true" label="{i18n>Esercizio}"
							visibleInFilterBar="true">
								<fb:control>
									<Input class="esercizioClass" width="20%" id="idEserGestione" value="2023" editable="false" textAlign="Center"/>
										<!-- <Text text="2023"/> -->
								</fb:control>
							</fb:FilterGroupItem>
							<fb:FilterGroupItem mandatory="true" groupName="Essential" groupTitle="{i18n>Essential}" id="filtroSottostrumento" name="A1" label="{i18n>Sottostrumento}"
								visibleInFilterBar="true">
								<fb:control>
									<!-- sostituito onSottostrumento con onHelpValueSottoStrumento -->
									<Input value="{modelPosFin>/Sottostrumento}" width="50%" showValueHelp="false" required="false"  editable="false" />
										<!-- <Link text="{modelHome>/Sottostrumento}" press="onExpandPopOverSottostrumento" class="sapUiSmallMarginEnd"/> -->
								</fb:control>
							</fb:FilterGroupItem>
							<fb:FilterGroupItem groupName="Essential" id="POSIZIONEFINANZIARIA" groupTitle="{i18n>ricercaStru}" name="A2" label="{i18n>posFin}"
