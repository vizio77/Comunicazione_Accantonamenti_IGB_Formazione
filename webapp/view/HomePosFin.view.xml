								<fb:control>
									<Input id="idPosFinF" value="{modelFilterHome>/PosizioneFinanziaria}" width="50%" showValueHelp="true" enabled="{modelFilterHome>/FieldPosEnabled}" 
										valueHelpRequest="onPosFin"/>
								</fb:control>
							</fb:FilterGroupItem>						
							
							<fb:FilterGroupItem groupName="posFin" groupTitle="{i18n>labelPosFin}" name="E" label="{i18n>Amministrazione}" partOfCurrentVariant="true" visibleInFilterBar="false">
								<fb:control>
									<Input id="idAmmFraa" value="{modelFilterHome>/Amministrazione}" showValueHelp="true" valueHelpRequest="matchCodeAmministrazione"/>
								</fb:control>
							</fb:FilterGroupItem>
							<fb:FilterGroupItem groupName="posFin" groupTitle="{i18n>labelPosFin}" id="filtroCapitolo" name="A" label="{i18n>Capitolo}"  partOfCurrentVariant="true"
								visibleInFilterBar="false">
								<fb:control>
									<Input id="idCapitolo" value="{modelFilterHome>/Capitolo}" width="50%"/>
								</fb:control>
							</fb:FilterGroupItem>
							<fb:FilterGroupItem groupName="posFin" groupTitle="{i18n>labelPosFin}" id="filtroPg" name="B" label="{i18n>Pg}"  partOfCurrentVariant="true"
								visibleInFilterBar="false">
								<fb:control>
									<Input id="idPg" value="{modelFilterHome>/Pg}" width="50%"/>
								</fb:control>
							</fb:FilterGroupItem>

							
						</fb:filterGroupItems>
