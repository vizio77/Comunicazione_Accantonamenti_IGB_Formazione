														</cells>
													</ColumnListItem>
												</items>
											</Table>
										</Panel>															
										<!-- Visualizza Collegamenti -->
										<Panel expandable="true" visible="{= ${modelPosFin>/onCreate} === false}" expanded="false">
											<headerToolbar>
												<OverflowToolbar>
													<Title text="{i18n>collegamenti}"/>
												</OverflowToolbar>
											</headerToolbar>
											<Table id="tableCollegamenti" width="auto">
												<columns>
													<Column width="5em">
														<Text text="Anno"/>
													</Column>
													<Column width="10em">
														<Text text="{i18n>Amministrazione}"/>
													</Column>
													<Column width="7em">
														<Text text="{i18n>Capitolo}"/>
													</Column>
													<Column width="7em">
														<Text text="{i18n>Pg}"/>
													</Column>
												</columns>
												<items>
													<ColumnListItem >
														<cells>
															<Text text="2022"/>
															<Text text="A020"/>
															<Text text="1001"/>
															<Text text=""/>
														</cells>
													</ColumnListItem>
												</items>
											</Table>
										</Panel>
									</Panel>
								</IconTabFilter>
								<IconTabFilter icon="sap-icon://process" key="attachments" text="{i18n>competenza}" id="idCompetenzaTab" visible="{= ${modelPosFin>/infoSottoStrumento/TipoEsposizione} !== '2'  &amp;&amp; ${modelPosFin>/onModify} === true }" >
									<FlexBox justifyContent="SpaceAround" width="40%">
										<items>
											<VBox>
												<Label text="Autorizzazione:" labelFor="idAuthComp"/>
												<Input value="{modelPosFin>/CompetenzaAuth/Auth}" showValueHelp="true" valueHelpRequest=".onAuth" id="idAuthComp" valueHelpOnly="true" width="130%">
												</Input>
											</VBox>
											<VBox>
												<Label text="Autorizzazione Collegata:" labelFor="idAuthAssComp"/>
												<Input value="{modelPosFin>/CompetenzaAuth/AuthAssociata}" showValueHelp="true" valueHelpRequest=".onAuthCollegata" id="idAuthAssComp" valueHelpOnly="true" width="130%">
												</Input>
											</VBox>
										</items>
									</FlexBox>
									<OverflowToolbar id="autorizzazioniCompButton" class="sapUiSmallMarginBottom" style="Clear">
										<ToolbarSpacer/>
										<Button text="{i18n>gestCondingBlock}" type="Emphasized" enabled="{= !!${modelPosFin>/CompetenzaAuth/Auth} }" press=".onGestisciCodingBlock"/>
										<Button text="{i18n>associaNuovaAuth}" type="Emphasized" press=".onNuovaAuth"/>
										<Button text="{i18n>resetta}" type="Emphasized" press="onResetAuth"/>
										<Button text="{i18n>avvia}" type="Emphasized"/>
									</OverflowToolbar>
									<html:iframe id ="attachmentframe" height="100%" width="100%" src=""> </html:iframe>
								</IconTabFilter>
								<IconTabFilter icon="sap-icon://accounting-document-verification" key="people" text="{i18n>cassa}"  id="idCassTab" visible="{= ${modelPosFin>/infoSottoStrumento/TipoEsposizione} !== '2' &amp;&amp; ${modelPosFin>/onModify} === true }"></IconTabFilter>
								<IconTabFilter icon="sap-icon://commission-check" key="RimOrizzontali" text="{i18n>RimOrizzontali}" visible="{= ${modelPosFin>/infoSottoStrumento/TipoEsposizione} === '2'  &amp;&amp; ${modelPosFin>/onModify} === true }">
									<FlexBox justifyContent="Start" width="40%" class="sapUiSmallMarginBegin">
										<items>
											<VBox>
												
												<Label text="Autorizzazione:" labelFor="idAuthCompRimO"/>
												<Input value="{modelPosFin>/CompetenzaAuth/Auth}" showValueHelp="true" valueHelpRequest=".onAuth" id="idAuthCompRimO" valueHelpOnly="true" width="130%">
												</Input>
											</VBox>
										</items>
									</FlexBox>
									<OverflowToolbar id="autorizzazioniCompButtonRimor" class="sapUiSmallMarginBottom" style="Clear">
										<ToolbarSpacer/>
										<Button text="{i18n>associaNuovaAuth}" type="Emphasized" press=".onNuovaAuth"/>
										<Button text="{i18n>resetta}" type="Emphasized" press="onResetAuth"/>
										<Button text="{i18n>avvia}" type="Emphasized"/>
									</OverflowToolbar>
									<html:iframe id ="attachmentframeRimOr" height="100%" width="100%" src=""> </html:iframe>
								</IconTabFilter>
							</items>
						</IconTabBar>
					</VBox>
				</f:content>
		<f:footer>
			<OverflowToolbar>
				<ToolbarSpacer/>
				<Button text="Torna Indietro" press="onNavBack" type="Emphasized" visible="true"/>
				<Button text="Salva" type="Emphasized" visible="{= ${modelPosFin>/onAvvio} === true  &amp;&amp; ${modelPosFin>/tabAnagrafica} === true}"/>
			</OverflowToolbar>
		</f:footer>
	</f:DynamicPage>
</App>
</mvc:View>